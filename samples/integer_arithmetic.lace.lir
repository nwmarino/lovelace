main :: external () -> i64 {
	x := i64 |8
	y := i64 |8
	z := i64 |8
bb0:
	store 42: i64, x: *i64
	$1 = load x: *i64 |8
	$2 = iadd $1: i64, 21: i64
	store $2: i64, y: *i64
	$3 = load x: *i64 |8
	$4 = isub $3: i64, 21: i64
	store $4: i64, z: *i64
	$5 = load x: *i64 |8
	$6 = load y: *i64 |8
	$7 = imul $5: i64, $6: i64
	$8 = load z: *i64 |8
	$9 = sdiv $7: i64, $8: i64
	ret $9: i64
}
