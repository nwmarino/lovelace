LIR_CONTROL_FLOW_GRAPH "/home/nwm/statim/samples/until_loop.lace"

main :: external () -> i64 {
	i := i64 |8
	x := i8 |1
bb0:
	store 0: i64, i: *i64
	store 0: i8, x: *i8
	jmp bb1
bb1:
	$1 = load x: *i8 |1
	$2 = cmp ieq $1: i8, 1: i8
	jif $2: i1, bb5, bb2
bb2:
	$3 = load i: *i64 |8
	$4 = cmp ieq $3: i64, 5: i64
	jif $4: i1, bb3, bb4
bb3:
	jmp bb5
bb4:
	$5 = load i: *i64 |8
	store $5: i64, i: *i64
	$6 = iadd $5: i64, 1: i64
	jmp bb1
bb5:
	$7 = load i: *i64 |8
	ret $7: i64
}
